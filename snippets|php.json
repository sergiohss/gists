	"die and Dump": {
		"prefix": "dd",
		"body": [
			"dd($1);"
		],
		"description": "die and dump"
	},

	
	"new public method": {
		"prefix": "pubf",
		"body": [
			"public function ${selector}()",
			"{",
			"    ",
			"}"
		],
		"description": "new public method"
	},


	"new private method": {
		"prefix": "prif",
		"body": [
			"private function ${selector}()",
			"{",
			"    ",
			"}"
		],
		"description": "new private method"
	},


	"new protected method": {
		"prefix": "prof",
		"body": [
			"protected function ${selector}()",
			"{",
			"    ",
			"}"
		],
		"description": "new protected method"
	},


	"new funcition": {
		"prefix": "func",
		"body": [
			"function ${selector}()",
			"{",
			"    ",
			"}"
		],
		"description": "new funcition"
	},

	"die and pint_r": {
		"prefix": "diep",
		"body": [
			"echo '<pre>';",
			"die(print_r($1));"
		],
		"description": "die and dump"
	},

}